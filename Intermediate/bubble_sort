#Code for BUBBLE SORT
def bubbleSort(ar,t):
  for i in range(t):
    for j in range(0, t-i-1):
       if ar[j] > ar[j+1] :
          ar[j], ar[j+1] = ar[j+1], ar[j]
 
ar =[]
t = int(input("Enter number of elements"))
for k in range(t):
    arr.append(t)
 
bubbleSort(ar,len(ar))
 
print ("Sorted array:")
for i in range(len(ar)):
    print ("%d" %ar[i]) 
